LETS_ENCRYPT_SERVICES+=calibre sabnzbd sabnzbd_api tautulli medusa ombi web_site web_newsletter transmission_internal transmission_external organizr2 plex mkvtoolnix calibreweb_books booksonic lazylibrarian

