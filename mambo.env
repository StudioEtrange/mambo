NETWORK_SERVICES_AREA_MAIN=organizr2 plex web sabnzbd tautulli medusa ombi transmission mkvtoolnix calibreweb_books booksonic calibre lazylibrarian
NETWORK_SERVICES_AREA_SECONDARY=
NETWORK_REDIRECT_HTTPS=enable
